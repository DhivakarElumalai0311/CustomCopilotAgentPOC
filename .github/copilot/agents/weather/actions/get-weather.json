{
  "name": "getWeather",
  "description": "Get weather using Open-Meteo public API",
  "input_schema": {
    "type": "object",
    "properties": {
      "city": { "type": "string" }
    },
    "required": ["city"]
  },
  "output_schema": {
    "type": "object",
    "properties": {
      "city": { "type": "string" },
      "temperature": { "type": "number" },
      "condition": { "type": "string" }
    }
  },
  "runtime": {
    "path": "get-weather.js"
  }
}
